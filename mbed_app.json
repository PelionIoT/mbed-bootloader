            "sotp-section-1-size"              : "(4*1024)",
            "sotp-section-2-address"           : "(MBED_CONF_APP_FLASH_START_ADDRESS+36*1024)",
            "sotp-section-2-size"              : "(4*1024)",
            "update-client.application-details": "(MBED_CONF_APP_FLASH_START_ADDRESS+40*1024)",
